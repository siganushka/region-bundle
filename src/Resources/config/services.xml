<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Siganushka\RegionBundle\Command\RegionUpdateCommand" autowire="true">
            <tag name="console.command" />
        </service>
        <service id="Siganushka\RegionBundle\Controller\RegionController" autowire="true">
            <tag name="controller.service_arguments" />
        </service>
        <service id="Siganushka\RegionBundle\Serializer\Normalizer\RegionNormalizer" autowire="true">
            <tag name="serializer.normalizer" />
        </service>
        <service id="Siganushka\RegionBundle\Form\Type\RegionProvinceType" autowire="true">
            <tag name="form.type" />
        </service>
        <service id="Siganushka\RegionBundle\Form\Type\RegionCityType" autowire="true">
            <tag name="form.type" />
        </service>
        <service id="Siganushka\RegionBundle\Form\Type\RegionDistrictType" autowire="true">
            <tag name="form.type" />
        </service>
        <service id="Siganushka\RegionBundle\Form\Type\RegionSubjectType" autowire="true">
            <tag name="form.type" />
        </service>
    </services>
</container>
