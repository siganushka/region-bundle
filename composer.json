{
    "name": "siganushka/region-bundle",
    "description": "Region bundle for symfony.",
    "keywords": ["region-bundle"],
    "homepage": "https://github.com/siganushka/region-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.11",
        "siganushka/doctrine-contracts": "^0.1",
        "siganushka/generic-bundle": "^0.6",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\RegionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\RegionBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
